CREATE table Groessen (
Paketgroesse varchar(3),
Min_Gurtmass int,
Max_Gurtmass int
);

INSERT INTO Groessen(Paketgroesse, Min_Gurtmass, Max_Gurtmass) 
values ('XS', 0, 35),
('S',35,50),
('M',50,65),
('L',65,80),
('XL',80,300);

SELECT Paketgroesse from Groessen where Min_Gurtmass >= 20 AND Max_Gurtmass < 20;



